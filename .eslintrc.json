{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes" : [2,"single", {"avoidEscape" : true}],
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off"
    }
}
